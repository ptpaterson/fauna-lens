{
  "name": "faunadb-lens",
  "version": "0.1.0",
  "description": "Collection of lens functions in order to use Shades with FaunaDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "testw": "jest --watch",
    "format": "prettier \"src/**/*.{js,ts}\" --write",
